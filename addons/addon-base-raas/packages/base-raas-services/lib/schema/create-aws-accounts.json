{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "name": {
        "type": "string",
        "minLength": 1,
        "maxLength": 100
      },
      "description": {
        "type": "string",
        "maxLength": 2048
      },
      "accountId": {
        "type": "string",
        "pattern": "^[0-9]{12}$"
      }
    },
    "required": ["name", "accountId"]
  }
  